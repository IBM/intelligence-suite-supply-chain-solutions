        {
            "name": "queries[0].fields",
            "values": [
                "query weeklyDemandOfOverConsumption($tenantId: String, $locationNameFilter: BooleanExp = {CONSTANT_VALUE: true}, $locationIDFilter: BooleanExp = {CONSTANT_VALUE: true}, $categoryFilter: BooleanExp = {CONSTANT_VALUE: true}, $plannerCodeFilter: BooleanExp = {CONSTANT_VALUE: true} ) {    OverConsumption: businessObjects(   simpleFilter: {tenantId: $tenantId, type: Inventory}  hint: { viewId: \"graph\" }  advancedFilter: {  AND: [  {  GREATER_THAN: [  { SELECT: \"customAttributes.consumptionFloat\", type: STRING },  { SELECT: \"customAttributes.consumptionUpperLimitFloat\", type: STRING }  ]  }, $locationNameFilter, $locationIDFilter, $categoryFilter, $plannerCodeFilter  ]  }) { totalCount }      AllInventory: businessObjects(   simpleFilter: {tenantId: $tenantId, type: Inventory} advancedFilter: {  AND: [ $locationNameFilter, $locationIDFilter, $categoryFilter, $plannerCodeFilter  ]  } hint: { viewId: \"graph\" }  ) { totalCount }}"
            ],
            "metaInformation": "infohub.fields"
        },
        {
            "name": "queries[0].variables.locationNameFilter",
            "values": [
                "{ \"EQUALS_ANY\": [ {\"SELECT\": \"location.locationName\", \"type\": \"STRING\" }, { \"VALUES\": {{ weeklyDemand.appliedFilters.LocationName.ids }}, \"type\": \"STRING\" } ] }"
            ],
            "metaInformation": "infohub.variable.dynamicExpr"
        },
