        "query latePlannedShipmentMetrics( $tenantId: String, $customerNameFilter: BooleanExp = {CONSTANT_VALUE: true}, $customerIdFilter: BooleanExp = {CONSTANT_VALUE: true}, $supplierNameFilter: BooleanExp = {CONSTANT_VALUE: true}, $supplierIdFilter: BooleanExp = {CONSTANT_VALUE: true}, $categoryFilter: BooleanExp = { CONSTANT_VALUE: true }, $originNameFilter: BooleanExp = {CONSTANT_VALUE: true}, $originIdFilter: BooleanExp = {CONSTANT_VALUE: true}, $destinationNameFilter: BooleanExp = {CONSTANT_VALUE: true}, $destinationIdFilter: BooleanExp = {CONSTANT_VALUE: true}) { totalLatePlannedShipment: businessObjects( advancedFilter: { AND: [ { GREATER_THAN: [ { SELECT: \"plannedShipDelay\", type: FLOAT }, { VALUE: \"0.0\", type: FLOAT } ] }, { OR: [ { EQUALS: [ { SELECT: \"shipmentStatusByDate\", type: STRING }, { type: STRING, VALUE: \"NOT_SHIPPED\" } ] }, { EQUALS: [ { SELECT: \"shipmentStatusByDate\", type: STRING }, { type: STRING, VALUE: \"PARTIALLY_SHIPPED\" } ] } ] }, { EQUALS: [ { SELECT: \"order.orderType\", type: STRING }, { type: STRING, VALUE: \"INBOUND\" } ] }, $customerNameFilter, $customerIdFilter, $supplierNameFilter, $supplierIdFilter, $categoryFilter, $originNameFilter, $originIdFilter, $destinationNameFilter, $destinationIdFilter ] }, cursorParams: { first: 0 }, hint: { viewId: \"graph\" }, simpleFilter: { tenantId: $tenantId, type: OrderLine } ) { totalCount } }"
